{
    "manifest_version": 3,
    "name": "InstaFlex Extension",
    "version": "1.1",
    "author": "Felix Pieber",
    "description": "A Dark Mode, Downloader and Hotkeys(?) Extension for Instagram.com",
    "update_url": "https://github.com/felixpieber/InstaFlex",

    "icons": {
        "72": "images/icons/icon-72x72.png",
        "96": "images/icons/icon-96x96.png",
        "128": "images/icons/icon-128x128.png"
    },
    
    "content_scripts": [
        {
            "matches": [
                "https://www.instagram.com/*", "https://about.instagram.com/*", "https://help.instagram.com/*", "https://business.facebook.com/creatorstudio"
            ],
            "js": ["content.js"]
        }
    ]
}